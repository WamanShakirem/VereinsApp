<Window x:Class="VereinsApp.Uebersicht"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VereinsApp"
        mc:Ignorable="d"
        Title="Uebersicht" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="PersonenDatenGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="1,62,-1,-62"/>
        <Label Content="Suche" HorizontalAlignment="Left" Margin="611,33,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBox_Search" HorizontalAlignment="Left" Margin="658,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_Search_TextChanged"/>

    </Grid>
</Window>
